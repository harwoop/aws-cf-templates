                                            " /tmp/create_ah_admin_user.sql"
                                        ]
                                    ]
                                }, 
                                {
                                    "Fn::Join": [
                                        "", 
                                        [
                                            "/usr/bin/mysql -u root -p", 
                                            {
                                                "Ref": "DBPassword"
                                            }, 
                                            " -h ", 
                                            {
                                                "Ref": "MySQLDatabase"
                                            }, 
                                            " </tmp/create_ah_admin_user.sql"
                                        ]
                                    ]
                                }, 
