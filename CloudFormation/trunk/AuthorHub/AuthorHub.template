                                {
                                    "Fn::Join": [
                                        "", 
                                        [
                                            "/usr/bin/mysql -u root -p", 
                                            {
                                                "Ref": "DBPassword"
                                            }, 
                                            " -h ", 
                                            {
                                                "Ref": "MySQLDatabase"
                                            }, 
                                            " <<EOD"
                                        ]
                                    ]
                                }, 
                                "use authorhub;", 
                                "source /tmp/authorhub.sql;", 
                                "exit;",
                                "EOD"
                            ]
                        ]
                    }
                }
