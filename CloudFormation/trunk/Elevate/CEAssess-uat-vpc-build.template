                                "/bin/mkdir /usr/java", 
                                "/bin/ln /usr/lib/jvm/jre /usr/java/default", 
                                "cd /tmp", 
                                "/usr/bin/curl \"https://bootstrap.pypa.io/get-pip.py\" -o \"get-pip.py\"", 
                                "/usr/bin/python get-pip.py", 
                                "/usr/bin/pip install awscli", 
                                "/usr/bin/aws s3 cp s3://cup-cea-files/cea-live-key.pem /home/ec2-user/.ssh/cea-live-key.pem", 
                                "/usr/bin/aws s3 cp s3://cup-cea-files/cea-config /home/ec2-user/.ssh/config", 
                                "/bin/chown -R ec2-user:ec2-user /home/ec2-user/.ssh", 
                                "/bin/chmod 700 /home/ec2-user/.ssh", 
                                "/bin/chmod 600 /home/ec2-user/.ssh/*",				
                                "/usr/bin/aws s3 cp s3://cup-cea-files/cea-dev-key.pem /home/ceadev/.ssh/cea-dev-key.pem", 
