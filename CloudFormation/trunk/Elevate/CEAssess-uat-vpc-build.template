								"/usr/bin/yum install -y mysql",
                                "/usr/sbin/groupadd -g 522 ceadev", 
                                "/usr/sbin/useradd -u 521 -g ceadev -c \"CEA Developer account\" ceadev", 
                                "cd /home/ceadev", 
                                "/bin/mkdir .ssh", 
